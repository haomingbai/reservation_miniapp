.beta-item::after {
  content: "Beta";
  background-color: var(--red);
  color: white;
  border-radius: 12rpx;
  font-size: 20rpx;
  padding: 0 10rpx;
  display: flex;
  align-items: center;
  height: 24rpx;

  position: absolute;
  bottom: 19rpx;
  right: 17%;
  animation-name: beat;
  /*动画名称*/
  animation-duration: .7s;
  animation-timing-function: ease-in-out;
  animation-iteration-count: infinite;
  animation-direction: alternate;
  /* animation-play-state: running; */
}
@keyframes beat {
  0% {
      bottom: 22rpx;
  }
  100% {
      bottom: 29rpx;
  }
}
.btn-container {
  display: flex;
  align-items: center;
  justify-content: space-around;
  width: 100%;
  height: calc(30vh - 152rpx);
}

.bottom-warp .btn-item {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 140rpx;
  width: 140rpx;
}

.take-btn {
  border-radius: 50%;
  border: 1rpx solid var(--theme);
  padding: 8rpx;
  overflow: hidden;
  justify-content: center;
  align-items: center;
}

.take-btn-in {
  border-radius: 50%;
  width: 100%;
  height: 100%;
  background-color: var(--theme);
}

.sub-btn {
  height: 100rpx !important;
  width: 100rpx !important;
}

image {
  width: 100%;
  height: 100%;
