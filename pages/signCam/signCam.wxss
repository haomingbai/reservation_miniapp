.camera-warp {
  position: relative;
}

.header-tool {
  position: absolute;
}

.bar-warp {
  display: flex;
  align-items: center;
  color: white;
}

.catch-warp {
  position: absolute;
  height: 70vh;
  width: 100vw;
  margin: 13vh 0vw 13vh;
  display: flex;
  align-items: center;
  justify-content: center;
  /* background-color: white; */
  /* border: 2rpx dotted #f2f2f2; */
}

.catch-tips {
  position: absolute;
  bottom: 5vh;
  color: #f2f2f2;
  width: 750rpx;
  text-align: center;
  font-size: 28rpx;
}

.identifying-img {
  width: 750rpx;
  height: 70vh;
}

.bottom-warp {
  position: fixed;
  bottom: 0;
  width: 750rpx;
  height: 30vh;
}

.bottom-bar {
  height: 76rpx;
  display: flex;
  align-items: center;
  /* justify-content: center; */
  color: #999;
  /* text-decoration: underline; */
  width: 100%;
  font-size: 25rpx;
}

.bottom-bar .bar-item {
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
  height: 100%;
  position: relative;
}

.bar-item__active {
  border-bottom: 3rpx solid var(--theme);
  font-size: 28rpx;
  color: var(--theme);
}

